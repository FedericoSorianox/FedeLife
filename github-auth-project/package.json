{
  "name": "github-auth-project",
  "version": "1.0.0",
  "description": "A Visual Studio Code extension for GitHub authentication.",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "keywords": [
    "vscode",
    "extension",
    "github",
    "authentication"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "vscode": "^1.1.37"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onCommand:extension.authenticate"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.authenticate",
        "title": "Authenticate with GitHub"
      }
    ]
  }
}